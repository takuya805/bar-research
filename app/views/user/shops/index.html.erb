
<% @shops.each do |shop| %>
<%= link_to shop.name, shop_path(shop) %><br />
<%= shop.category.category %><br />
<%= attachment_image_tag(shop, :image, size: '100x100', format: 'jpg') %><br />
<%= shop.explain %><br />
<%= shop.station %><br />
<%= shop.budget %><br />
<%= shop.seat %><br />
<%= shop.phone %>
<% end %>